{
    "openapi": "3.1.0",
    "info": {
        "title": "Laravel",
        "version": "0.0.1"
    },
    "servers": [
        {
            "url": "http://localhost/api"
        }
    ],
    "security": [
        {
            "laravel-session": []
        },
        {
            "csrf_token": []
        }
    ],
    "paths": {
        "/dto-json-response": {
            "get": {
                "operationId": "test.dto-json-response",
                "description": "Displays a JSON response with a SimpleDto object, inference not supported without a custom-to-schema extension",
                "summary": "Dto response",
                "tags": [
                    "DtoJsonResponse"
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "object"
                                        }
                                    },
                                    "required": [
                                        "data"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/dto-resource-response": {
            "get": {
                "operationId": "test.dto-resource-response",
                "description": "Displays a JSON resource with a SimpleDto object.",
                "summary": "Dto response",
                "tags": [
                    "DtoResource"
                ],
                "responses": {
                    "200": {
                        "description": "`DtoResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/DtoResource"
                                        }
                                    },
                                    "required": [
                                        "data"
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/list-products": {
            "get": {
                "operationId": "test.list-products",
                "description": "Displays a listing of products.",
                "summary": "List products",
                "tags": [
                    "PaginatedProduct"
                ],
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "The current page number",
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "type": "object",
                                            "description": "Lier\\ScrambleExtensions\\Appendable\\AppendableAnonymousJsonResourceCollectionSchema",
                                            "properties": {
                                                "data": {
                                                    "type": "array",
                                                    "items": {
                                                        "$ref": "#/components/schemas/ProductResource"
                                                    }
                                                }
                                            },
                                            "required": [
                                                "data"
                                            ]
                                        },
                                        {
                                            "$ref": "#/components/schemas/LengthAwarePaginator"
                                        }
                                    ]
                                }
                            }
                        }
                    }
                }
            }
        },
        "/products-with-filters": {
            "get": {
                "operationId": "test.products-with-filters",
                "description": "Displays a listing of products.",
                "summary": "List products",
                "tags": [
                    "ProductWithComplexFilters"
                ],
                "parameters": [
                    {
                        "name": "string_rule",
                        "in": "query",
                        "schema": {
                            "type": "string",
                            "maxLength": 255
                        }
                    },
                    {
                        "name": "array_rule",
                        "in": "query",
                        "schema": {
                            "type": "string",
                            "minLength": 1,
                            "maxLength": 255
                        },
                        "example": "'john doe'"
                    },
                    {
                        "name": "in_rule",
                        "in": "query",
                        "schema": {
                            "type": "string",
                            "enum": [
                                "john",
                                "doe"
                            ]
                        }
                    },
                    {
                        "name": "names",
                        "in": "query",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "description": "Lier\\ScrambleExtensions\\Appendable\\AppendableAnonymousJsonResourceCollectionSchema",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/ProductResource"
                                            }
                                        }
                                    },
                                    "required": [
                                        "data"
                                    ]
                                }
                            }
                        }
                    },
                    "422": {
                        "$ref": "#/components/responses/ValidationException"
                    }
                }
            }
        },
        "/basic-user-crud": {
            "get": {
                "operationId": "test.basic-user-crud.index",
                "tags": [
                    "UserWithAppends"
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "description": "Lier\\ScrambleExtensions\\Appendable\\AppendableAnonymousJsonResourceCollectionSchema",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "allOf": [
                                                    {
                                                        "$ref": "#/components/schemas/UserResource"
                                                    },
                                                    {
                                                        "type": "object",
                                                        "properties": {
                                                            "posts": {
                                                                "type": "array",
                                                                "description": "Lier\\ScrambleExtensions\\Appendable\\AppendableAnonymousJsonResourceCollectionSchema",
                                                                "items": {
                                                                    "allOf": [
                                                                        {
                                                                            "$ref": "#/components/schemas/PostResource"
                                                                        },
                                                                        {
                                                                            "type": "object",
                                                                            "properties": {
                                                                                "author": {
                                                                                    "$ref": "#/components/schemas/UserResource"
                                                                                },
                                                                                "liked_by_users": {
                                                                                    "type": "array",
                                                                                    "description": "Lier\\ScrambleExtensions\\Appendable\\AppendableAnonymousJsonResourceCollectionSchema",
                                                                                    "items": {
                                                                                        "$ref": "#/components/schemas/UserResource"
                                                                                    }
                                                                                }
                                                                            },
                                                                            "required": [
                                                                                "liked_by_users"
                                                                            ]
                                                                        }
                                                                    ]
                                                                }
                                                            }
                                                        },
                                                        "required": [
                                                            "posts"
                                                        ]
                                                    }
                                                ]
                                            }
                                        },
                                        "names": {
                                            "type": "string"
                                        }
                                    },
                                    "required": [
                                        "data",
                                        "names"
                                    ]
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "operationId": "test.basic-user-crud.store",
                "tags": [
                    "UserWithAppends"
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/StoreUserRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "`UserResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/UserResource"
                                        },
                                        "authorizations": {
                                            "type": "object",
                                            "properties": {
                                                "users": {
                                                    "type": "object",
                                                    "properties": {
                                                        "view": {
                                                            "type": "boolean"
                                                        },
                                                        "update": {
                                                            "type": "boolean"
                                                        },
                                                        "delete": {
                                                            "type": "boolean"
                                                        }
                                                    },
                                                    "required": [
                                                        "view",
                                                        "update",
                                                        "delete"
                                                    ]
                                                }
                                            },
                                            "required": [
                                                "users"
                                            ]
                                        }
                                    },
                                    "required": [
                                        "data",
                                        "authorizations"
                                    ]
                                }
                            }
                        }
                    },
                    "422": {
                        "$ref": "#/components/responses/ValidationException"
                    },
                    "403": {
                        "$ref": "#/components/responses/AuthorizationException"
                    }
                }
            }
        },
        "/basic-user-crud/{basic_user_crud}": {
            "get": {
                "operationId": "test.basic-user-crud.show",
                "tags": [
                    "UserWithAppends"
                ],
                "parameters": [
                    {
                        "name": "basic_user_crud",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "description": "Lier\\ScrambleExtensions\\Appendable\\AppendableJsonResourceToSchema",
                                    "properties": {
                                        "data": {
                                            "allOf": [
                                                {
                                                    "$ref": "#/components/schemas/UserResource"
                                                },
                                                {
                                                    "type": "object",
                                                    "properties": {
                                                        "name": {
                                                            "$ref": "#/components/schemas/Role"
                                                        }
                                                    },
                                                    "required": [
                                                        "name"
                                                    ]
                                                }
                                            ]
                                        }
                                    },
                                    "required": [
                                        "data"
                                    ]
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "operationId": "test.basic-user-crud.update",
                "tags": [
                    "UserWithAppends"
                ],
                "parameters": [
                    {
                        "name": "basic_user_crud",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/UpdateUserRequest"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "`UserResource`",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/components/schemas/UserResource"
                                        },
                                        "authorizations": {
                                            "type": "object",
                                            "properties": {
                                                "users": {
                                                    "type": "object",
                                                    "properties": {
                                                        "view": {
                                                            "type": "boolean"
                                                        },
                                                        "update": {
                                                            "type": "boolean"
                                                        },
                                                        "delete": {
                                                            "type": "boolean"
                                                        }
                                                    },
                                                    "required": [
                                                        "view",
                                                        "update",
                                                        "delete"
                                                    ]
                                                }
                                            },
                                            "required": [
                                                "users"
                                            ]
                                        }
                                    },
                                    "required": [
                                        "data",
                                        "authorizations"
                                    ]
                                }
                            }
                        }
                    },
                    "422": {
                        "$ref": "#/components/responses/ValidationException"
                    },
                    "403": {
                        "$ref": "#/components/responses/AuthorizationException"
                    }
                }
            },
            "delete": {
                "operationId": "test.basic-user-crud.destroy",
                "tags": [
                    "UserWithAppends"
                ],
                "parameters": [
                    {
                        "name": "basic_user_crud",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "No content"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "laravel-session": {
                "type": "apiKey",
                "description": "The authenticated session cookie",
                "in": "cookie",
                "name": "laravel-session"
            },
            "csrf_token": {
                "type": "apiKey",
                "description": "A header containing the current CSRF token",
                "in": "header",
                "name": "X-CSRF-TOKEN"
            }
        },
        "schemas": {
            "DtoResource": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "age": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "age",
                    "role"
                ],
                "title": "DtoResource"
            },
            "LengthAwarePaginator": {
                "type": "object",
                "properties": {
                    "meta": {
                        "type": "object",
                        "properties": {
                            "current_page": {
                                "type": "integer"
                            },
                            "from": {
                                "type": "integer"
                            },
                            "last_page": {
                                "type": "integer"
                            },
                            "links": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "url": {
                                            "type": [
                                                "string",
                                                "null"
                                            ]
                                        },
                                        "label": {
                                            "type": "string"
                                        },
                                        "active": {
                                            "type": [
                                                "boolean",
                                                "null"
                                            ]
                                        }
                                    },
                                    "required": [
                                        "url",
                                        "label",
                                        "active"
                                    ]
                                }
                            },
                            "path": {
                                "type": "string"
                            },
                            "per_page": {
                                "type": "integer"
                            },
                            "to": {
                                "type": "integer"
                            },
                            "total": {
                                "type": "integer"
                            }
                        },
                        "required": [
                            "current_page",
                            "from",
                            "last_page",
                            "path",
                            "per_page",
                            "to",
                            "total"
                        ]
                    },
                    "links": {
                        "type": "object",
                        "properties": {
                            "first": {
                                "type": "string"
                            },
                            "last": {
                                "type": "string"
                            },
                            "prev": {
                                "type": "string"
                            },
                            "next": {
                                "type": "string"
                            }
                        }
                    }
                },
                "required": [
                    "meta",
                    "links"
                ],
                "title": "LengthAwarePaginator"
            },
            "Money": {
                "type": "object",
                "properties": {
                    "amount": {
                        "type": "string",
                        "description": "The amount formatted as a string. `<= 999.999.999.999,0000`",
                        "examples": [
                            "100000.2321"
                        ]
                    },
                    "currency": {
                        "type": "string",
                        "description": "A 3-letter uppercase ISO 4217 currency code.",
                        "examples": [
                            "USD"
                        ],
                        "minLength": 3,
                        "maxLength": 3
                    }
                },
                "required": [
                    "amount",
                    "currency"
                ],
                "title": "Money"
            },
            "PostResource": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "content": {
                        "type": "string"
                    },
                    "tags": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "created_at": {
                        "type": "string",
                        "format": "date-time"
                    },
                    "updated_at": {
                        "type": "string",
                        "format": "date-time"
                    }
                },
                "required": [
                    "title",
                    "content",
                    "created_at",
                    "updated_at"
                ],
                "title": "PostResource"
            },
            "ProductResource": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "price": {
                        "$ref": "#/components/schemas/Money"
                    }
                },
                "required": [
                    "title",
                    "price"
                ],
                "title": "ProductResource"
            },
            "Role": {
                "type": "string",
                "enum": [
                    "admin",
                    "user",
                    "guest"
                ],
                "title": "Role"
            },
            "StoreUserRequest": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "role": {
                        "$ref": "#/components/schemas/Role"
                    }
                },
                "required": [
                    "name",
                    "role"
                ],
                "title": "StoreUserRequest"
            },
            "UpdateUserRequest": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string",
                        "maxLength": 255
                    },
                    "prohibited_with_route_parameter": {
                        "type": [
                            "string",
                            "null"
                        ]
                    }
                },
                "required": [
                    "name"
                ],
                "title": "UpdateUserRequest"
            },
            "UserResource": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "role": {
                        "$ref": "#/components/schemas/Role"
                    }
                },
                "required": [
                    "name",
                    "role"
                ],
                "title": "UserResource"
            }
        },
        "responses": {
            "ValidationException": {
                "description": "Validation error",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "description": "Errors overview."
                                },
                                "errors": {
                                    "type": "object",
                                    "description": "A detailed description of each field that failed validation.",
                                    "additionalProperties": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    }
                                }
                            },
                            "required": [
                                "message",
                                "errors"
                            ]
                        }
                    }
                }
            },
            "AuthorizationException": {
                "description": "Authorization error",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "message": {
                                    "type": "string",
                                    "description": "Error overview."
                                }
                            },
                            "required": [
                                "message"
                            ]
                        }
                    }
                }
            }
        }
    }
}
